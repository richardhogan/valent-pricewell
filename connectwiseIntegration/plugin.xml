<plugin name='connectwise-integration' version='0.1' grailsVersion='2.0.0 &gt; *'>
  <author>Abhishek Bhandari</author>
  <authorEmail>abhi10901@gmail.com</authorEmail>
  <title>Connecwise To Pricewell</title>
  <description>\
This is a plugin for transfering connectwise data into pricewell.
</description>
  <documentation>http://grails.org/plugin/connectwise-integration</documentation>
  <type>ConnectwiseIntegrationGrailsPlugin</type>
  <resources>
    <resource>com.connectwise.integration.ConnectwiseExporterService</resource>
    <resource>com.connectwise.integration.ConnectwiseImporterService</resource>
    <resource>com.connectwise.integration.XTrustProvider</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://snapshots.repository.codehaus.org' url='http://snapshots.repository.codehaus.org/' />
    <repository name='http://repository.codehaus.org' url='http://repository.codehaus.org/' />
    <repository name='http://download.java.net/maven/2/' url='http://download.java.net/maven/2/' />
    <repository name='http://repository.jboss.com/maven2/' url='http://repository.jboss.com/maven2/' />
    <repository name='http://m2repo.spockframework.org/releases/' url='http://m2repo.spockframework.org/releases/' />
    <repository name='http://m2repo.spockframework.org/snapshots/' url='http://m2repo.spockframework.org/snapshots/' />
  </repositories>
  <dependencies>
    <compile>
      <dependency group='org.tmatesoft.svnkit' name='svnkit' version='1.3.5' />
      <dependency group='org.quartz-scheduler' name='quartz' version='1.8.4' />
      <dependency group='org.jsoup' name='jsoup' version='1.8.1' />
      <dependency group='ma.glasnost.orika' name='orika-core' version='1.4.0' />
      <dependency group='commons-lang' name='commons-lang' version='2.2' />
    </compile>
    <runtime>
      <dependency group='hsqldb' name='hsqldb' version='1.8.0.10' />
    </runtime>
  </dependencies>
  <plugins>
    <runtime>
      <plugin group='org.grails.plugins' name='navigation' version='1.2' />
      <plugin group='org.grails.plugins' name='resources' version='1.1.6' />
      <plugin group='org.grails.plugins' name='constraints' version='0.6.0' />
      <plugin group='org.grails.plugins' name='tomcat' version='2.0.0' />
      <plugin group='org.grails.plugins' name='ws-client' version='1.0' />
      <plugin group='org.grails.plugins' name='hibernate' version='2.0.0' />
      <plugin group='org.grails.plugins' name='shiro' version='1.0.1' />
      <plugin group='org.grails.plugins' name='mail' version='1.0' />
      <plugin group='org.grails.plugins' name='export' version='1.0' />
      <plugin group='org.grails.plugins' name='google-visualization' version='0.3' />
      <plugin group='org.grails.plugins' name='svn' version='1.0.2' />
      <plugin group='org.grails.plugins' name='remote-pagination' version='0.3' />
      <plugin group='org.grails.plugins' name='ckeditor' version='3.6.2.1' />
      <plugin group='org.grails.plugins' name='modalbox' version='0.4' />
      <plugin group='org.grails.plugins' name='prototype' version='1.0' />
      <plugin group='org.grails.plugins' name='cache-headers' version='1.1.5' />
    </runtime>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>